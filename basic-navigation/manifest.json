{

  "name": "Shortcuts for basic navigation",
  "description": "Allow to natively specify shortcuts for basic navigation in Chrome",
  "version": "1.0",
  "manifest_version": 2,

  "icons": {
    "16":  "images/icon-16px.png",
    "32":  "images/icon-32px.png",
    "64":  "images/icon-64px.png",
    "128": "images/icon-128px.png",
    "256": "images/icon-256px.png",
    "512": "images/icon-512px.png"
  },

  "background": {
    "scripts": [ "background.js" ]
  },

  "browser_action": {
    "default_title": "Open shortcuts bindings"
  },

  "permissions":[ "tabs"] ,

  "commands": {

    "next-tab": {
      "suggested_key": {
        "default": "Alt+Up"
      },
      "description": "Next tab"
    },
    "previous-tab": {
      "suggested_key": {
        "default": "Alt+Down"
      },
      "description": "Previous tab"
    },

    "go-forward": {
      "suggested_key": {
        "default": "Alt+Shift+Delete"
      },
      "description": "Go forward"
    },
    "go-back": {
      "suggested_key": {
        "default": "Alt+Delete"
      },
      "description": "Go back"
    }
  }

}
